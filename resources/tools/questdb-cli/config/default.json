{
  "database": {
    "host": "localhost",
    "httpPort": 9000,
    "ilpPort": 9009,
    "pgPort": 8812,
    "username": "admin",
    "password": "quest",
    "timeout": 30000,
    "connectOptions": {
      "application_name": "questdb-cli"
    }
  },
  "paths": {
    "outputDir": "./output",
    "templatesDir": "./templates",
    "scriptsDir": "./scripts",
    "logsDir": "./logs"
  },
  "defaults": {
    "format": "table",
    "limit": 100,
    "timezone": "UTC",
    "maxRetries": 3,
    "retryDelay": 1000
  },
  "api": {
    "baseUrl": "http://localhost:8080/api/v1",
    "timeout": 30000
  },
  "display": {
    "maxColumnWidth": 40,
    "truncateThreshold": 1000,
    "colorEnabled": true,
    "timestampFormat": "YYYY-MM-DD HH:mm:ss.SSS"
  }
}